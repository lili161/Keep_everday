    <TextView
            android:text="null"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/paobucishu" android:textSize="24sp"
            app:layout_constraintStart_toEndOf="@+id/textView6" app:layout_constraintEnd_toEndOf="parent"
